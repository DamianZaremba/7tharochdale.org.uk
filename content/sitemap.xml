---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc>{{ site.basedomain }}/</loc>
		<lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/scouts/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/news.atom</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/leaders/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/gallery/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/faq/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/explorers/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/cubs/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/contact/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/beavers/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/about/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/news/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/news/categories/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	<url>
		<loc>{{ site.basedomain }}/news/archives/</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>

	{% for post in site.posts %}
	<url>
		<loc>{{ site.basedomain }}/{{ post.url }}</loc>
		<lastmod>{{ site.time }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.2</priority>
	</url>
	{% endfor %}
</urlset>