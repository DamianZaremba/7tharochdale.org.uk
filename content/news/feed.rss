---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>{{ site.title }}</title>
	<link href="{{ site.basedomain }}/news/feed.atom" rel="self" />
	<link href="{{ site.basedomain }}/" />
	<updated>{{ site.time || date_to_xmlschema }}</updated>
	<id>{{ site.basedomain }}/</id>
	<author>
		<name>7th A Rochdale Scout Group</name>
		<email>group@7tharochdale.org.uk</email>
	</author>
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="{{ site.basedomain }}/{{ post.url }}" />
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>{{ site.basedomain }}/{{ post.id }}</id>
		<description>{{ post.content | strip_html | truncatewords: 35 | xml_escape }} [..]</description>
		<content type="html">
			{{ post.content | xml_escape }}
		</content>
	<author>
		<name>{{ post.author }}</name>
		<email>group@7tharochdale.org.uk</email>
	</author>
	</entry>
	{% endfor %}
</feed>
